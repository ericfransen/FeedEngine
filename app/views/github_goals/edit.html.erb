<div class = 'row panel github_panel_edit'>
  <h1>Edit your daily GitHub commit goal</h1>
  <div class = 'github-goal-edit'>

    <%= form_for(@github_goal) do |f| %>
      <%= f.label :number_of_commits %>
      <%= f.select :commit_goal, (1..27).to_a %>
      <%= f.submit 'Create Goal', class: 'button' %>
    <% end %>
  </div>
</div>
