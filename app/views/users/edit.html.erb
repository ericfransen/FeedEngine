<div class='github-goal-edit'>
  <%= form_for @user, url: user_path, method: :put do |f| %>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
    <%= f.label :when_do_you_want_to_recieve_your_daily_evening_reminder %>
    <%= f.select :time_to_text, [2, 5, 7] %>
    <%= f.submit 'submit' %>
  <% end %>
</div>
